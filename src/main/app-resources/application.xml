<?xml version="1.0" encoding="us-ascii"?>
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" id="proc-coreg">
  <jobTemplates>
    <jobTemplate id="proc-coreg">
      <streamingExecutable>/application/proc-coreg/run.sh</streamingExecutable>
      <defaultParameters>
        <!-- CreateStack -->
        <parameter id="resamplingType" title="resamplingType" abstract="resamplingType" scope="runtime" type="LiteralData">NONE</parameter>
        <parameter id="extent" title="extent" abstract="extent" scope="runtime" type="LiteralData">Master</parameter>
        <!-- GCP-Selection -->
        <parameter id="applyFineRegistration"></parameter>
        <parameter id="coarseRegistrationWindowHeight"></parameter>
        <parameter id="coarseRegistrationWindowWidth"></parameter>
        <parameter id="coherenceThreshold"></parameter>
        <parameter id="coherenceWindowSize"></parameter>
        <parameter id="columnInterpFactor"></parameter>
        <parameter id="computeOffset"></parameter>
        <parameter id="fineRegistrationWindowHeight"></parameter>
        <parameter id="fineRegistrationWindowWidth"></parameter>
        <parameter id="gcpTolerance"></parameter>
        <parameter id="maxIteration"></parameter>
        <parameter id="numGCPtoGenerate"></parameter>
        <parameter id="onlyGCPsOnLand"></parameter>
        <parameter id="rowInterpFactor"></parameter>
        <parameter id="useSlidingWindow"></parameter>
        <!-- Warp -->
        <parameter id="interpolationMethod"></parameter>
        <parameter id="openResidualsFile"></parameter>
        <parameter id="rmsThreshold"></parameter>
        <parameter id="warpPolynomialOrder"></parameter>
      </defaultParameters>
      <defaultJobconf>
        <property id="mapred.task.timeout">9900000000</property>
      </defaultJobconf>
    </jobTemplate>
  </jobTemplates>
  <workflow id="proc-coreg" title="SRTM Digital Elevation Model" abstract="This application generates the STRM DEM in ROI_PAC or GAMMA ready format">
